#include <stdio.h>
#include <stdlib.h>

int main(){

    int p1, p2, p3;
    float notaTrabalho, notaAvalia, notaExame, media;

    p1 = 2;
    p2 = 3;
    p3 = 5;

    printf("Nota do Trabalho de laboratorio: ");
    scanf("%f", &notaTrabalho);

    printf("Nota da Avaliacao semestral: ");
    scanf("%f", &notaAvalia);

    printf("Exame Final: ");
    printf("%f", &notaExame);

    media = (notaTrabalho * p1) + (notaAvalia * p2) + (notaExame * p3) / p1 + p2 +p3;

    printf("%f", media);

    return 0;
}
